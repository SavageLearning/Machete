          <DataField>UserLogin</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ConnectionString">
          <DataField>ConnectionString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
