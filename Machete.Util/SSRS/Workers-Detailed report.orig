    <DataSet Name="memberNumbers">
      <Query>
        <DataSourceName>macheteStageProd</DataSourceName>
        <CommandText>SELECT
  Workers.dwccardnum
  ,convert(varchar(5), Workers.dwccardnum) + '--' + Persons.firstname1 + ' ' + Persons.lastname1 as Name
FROM
  Persons
  INNER JOIN Workers
    ON Persons.ID = Workers.ID
    order by dwccardnum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="dwccardnum">
          <DataField>dwccardnum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
