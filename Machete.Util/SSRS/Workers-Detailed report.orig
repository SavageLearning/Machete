    <DataSet Name="workerDetails">
      <Query>
        <DataSourceName>macheteStageProd</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dwccardnum">
            <Value>=Parameters!dwccardnum.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     w.ID, 
			p.firstname1, 
			p.firstname2, 
			p.lastname1, 
			p.lastname2, 
			p.address1, 
			p.address2, 
			p.city, 
			p.state, 
			CASE WHEN p.zipcode IS NULL THEN 'unknown' ELSE p.zipcode END AS zipcode, 
			p.phone,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = p.gender)) AS Gender, p.genderother, 
                      CONVERT(VARCHAR(11), w.dateOfMembership, 100) AS dateOfMembership,
                      CONVERT(VARCHAR(11), w.memberReactivateDate, 100) AS memberReactivateDate,
                      CONVERT(VARCHAR(11), w.dateOfBirth, 100) AS dateOfBirth,                             
                      CASE p.[active] WHEN 0 THEN 'No' WHEN 1 THEN 'Yes' ELSE 'Unknown' END AS pActive,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.memberStatus)) AS memberStatus, 
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.RaceID)) AS Race, 
                            w.raceother, w.height, w.weight, w.englishlevelID AS englishlevel, 
                      CASE [recentarrival] WHEN 0 THEN 'No' WHEN 1 THEN 'Yes' ELSE 'Unknown' END AS recentarrival,
                      CONVERT(VARCHAR(11), w.dateinUSA, 100) AS dateinUSA,
                      CONVERT(VARCHAR(11), w.dateinseattle, 100) AS dateinseattle,                         
                      CASE [disabled] WHEN 0 THEN 'No' WHEN 1 THEN 'Yes' ELSE 'Unknown' END AS disabled, w.disabilitydesc,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.maritalstatus)) AS maritalstatus, 
                      CASE [livewithchildren] WHEN 0 THEN 'No' WHEN 1 THEN 'Yes' ELSE 'Unknown' END AS livewithchildren, 
                      w.numofchildren,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.incomeID)) AS income, 
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.typeOfWorkID)) AS typeOfWork,                             
                      CASE [livealone] WHEN 0 THEN 'No' WHEN 1 THEN 'Yes' ELSE 'Unknown' END AS livealone, w.emcontUSAname, 
                      w.emcontUSArelation, w.emcontUSAphone, w.dwccardnum,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.neighborhoodID)) AS neighborhood, CASE [immigrantrefugee] WHEN 0 THEN 'No' WHEN 1 THEN 'Yes' ELSE 'Unknown' END AS immigrantrefugee,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.countryoforiginID)) AS countryoforigin, w.emcontoriginname, w.emcontoriginrelation, w.emcontoriginphone, 
                      CONVERT(VARCHAR(11), w.memberexpirationdate, 100) AS memberexpirationdate,
                      CASE [driverslicense] WHEN 0 THEN 'No' WHEN 1 THEN 'Yes' ELSE 'Unknown' END AS driverslicense, 
                      CONVERT(VARCHAR(11), w.licenseexpirationdate, 100) AS licenseexpirationdate,
                      CASE [carinsurance] WHEN 0 THEN 'No' WHEN 1 THEN 'Yes' ELSE 'Unknown' END AS carinsurance, 
                      w.insuranceexpiration, 
                      im.ImageData,
                      im.ImageMimeType,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.skill1)) AS skill1,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.skill2)) AS skill2,
                          (SELECT     text_EN
                            FROM          dbo.Lookups AS l
                            WHERE      (ID = w.skill3)) AS skill3, 
                      w.datecreated AS woDateCreated, 
                      w.dateupdated AS woDateUpdated, 
                      w.Createdby AS woCreatedBy, 
                      w.Updatedby as woUpdatedby, 
                      p.datecreated AS pDateCreated, 
                      p.dateupdated AS pDateUpdated, 
                      p.Createdby AS pCreatedby, 
                      p.Updatedby AS pUpdatedby
                      
FROM dbo.Workers AS w INNER JOIN
     dbo.Persons AS p ON w.ID = p.ID LEFT JOIN
     dbo.images as im on im.ID = w.ImageID
where w.dwccardnum = @dwccardnum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
