        <QueryParameters>
          <QueryParameter Name="@dwccardnum">
            <Value>=Parameters!dwccardnum.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  wsi.WorkerID
  ,wsi.dateforsignin
FROM
  WorkerSignins wsi
where dwccardnum = @dwccardnum
union all select @dwccardnum as workerID, '1/1/1900' as dateforsignin
union all select @dwccardnum as workerID, '1/2/1900' as dateforsignin
union all select @dwccardnum as workerID, '1/3/1900' as dateforsignin
