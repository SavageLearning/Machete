        function CalculateWorkerFees() {
            var totalWorkerRates = 0;
            var workerCenter = "" + $("#workerCenter").val() + "";
            console.log(workerCenter);

            for (var i = 0; i < workAssignments.assignments.length; i++)
            {
                // Note: this is logic that is specific to Casa Latina only
                if (workerCenter == "Casa Latina") {
                    if (workAssignments.assignments[i].skillId == "67")
                    {
                        // This represents Casa's scale of hiring HHH workers at $65 for first
                        // 3 hours & an additional $16/hr afterwards
                        if (workAssignments.assignments[i].hours <= 3)
                        {
                            totalWorkerRates += 65;
                        }
                        else
                        {
                            // Note: charge a minimum of $18/hr if lifting weight is required
                            if (document.getElementById("Weight").checked && (workAssignments.assignments[i].hourlyWage < 18)) {
                                totalWorkerRates += 65 + ((workAssignments.assignments[i].hours - 3) * 18);
                            } else {
                                totalWorkerRates += 65 + ((workAssignments.assignments[i].hours - 3) * workAssignments.assignments[i].hourlyWage);
                            }
                        }
                    }               
                    else
                    {
                        if (document.getElementById("Weight").checked && (workAssignments.assignments[i].hourlyWage < 18)) {
                            totalWorkerRates += workAssignments.assignments[i].hours * 18;
                        } else {
                            totalWorkerRates += workAssignments.assignments[i].hours * workAssignments.assignments[i].hourlyWage; 
                        }
                    } 
                }
                else
                {
                    totalWorkerRates += workAssignments.assignments[i].hours * workAssignments.assignments[i].hourlyWage; 
                }
            }

            if (totalWorkerRates > 0)
            {
                $("#estimatedCosts").text("$" + totalWorkerRates.toFixed(2));
            } else {
                $("#estimatedCosts").text("TBD");
            }
        };

