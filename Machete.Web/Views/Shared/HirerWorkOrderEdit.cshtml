        function checkDate() {

            var userInput = $("#@(idPrefix)dateTimeofWork").val();
            var userInputInMs = new Date(userInput);

            /*
            http://stackoverflow.com/questions/5619202/converting-string-to-date-in-js
            Date.parse almost gets you what you want. It chokes on the am/pm part, but with some hacking you can get it to work:

            var str = 'Sun Apr 25, 2010 3:30pm',
                timestamp;

            timestamp = Date.parse(str.replace(/[ap]m$/i, ''));

            if(str.match(/pm$/i) >= 0) {
                timestamp += 12 * 60 * 60 * 1000;
            }
            */

            if (isNaN(userInputInMs.valueOf())) {
                $("#invalidDate").text("The date must be in a valid date form (e.g. 03/14/2015 14:00)").show();
                return;
            } else {
                $("#invalidDate").hide();
            }

            var dayOfWeek = userInputInMs.getDay();
            console.log("The day of week is: " + dayOfWeek);
            var hourOfDay = userInputInMs.getHours();
            console.log("The hour of day is: " + hourOfDay);

            // Set up Day of Week Conversion
            var weekday = new Array(7);
            weekday[0]=  "Sunday";
            weekday[1] = "Monday";
            weekday[2] = "Tuesday";
            weekday[3] = "Wednesday";
            weekday[4] = "Thursday";
            weekday[5] = "Friday";
            weekday[6] = "Saturday";

            var advanceHours;
            var earliestHour;
            var latestHour;

            if (dayOfWeek == 0) { // Sunday
                advanceHours = parseInt(@Model.def.getConfig("OnlineAdvanceHoursSunday"));
                earliestHour = parseInt(@Model.def.getConfig("OnlineEarliestHoursSunday"));
                latestHour = parseInt(@Model.def.getConfig("OnlineLatestHoursSunday"));
            } else if (dayOfWeek == 1) { // Monday
                advanceHours = parseInt(@Model.def.getConfig("OnlineAdvanceHoursMonday"));
                earliestHour = parseInt(@Model.def.getConfig("OnlineEarliestHoursMonday"));
                latestHour = parseInt(@Model.def.getConfig("OnlineLatestHoursMonday"));
            } else if (dayOfWeek == 2) { // Tuesday
                advanceHours = parseInt(@Model.def.getConfig("OnlineAdvanceHoursTuesday"));
                earliestHour = parseInt(@Model.def.getConfig("OnlineEarliestHoursTuesday"));
                latestHour = parseInt(@Model.def.getConfig("OnlineLatestHoursTuesday"));
            } else if (dayOfWeek == 3) { // Wednesday
                advanceHours = parseInt(@Model.def.getConfig("OnlineAdvanceHoursWednesday"));
                earliestHour = parseInt(@Model.def.getConfig("OnlineEarliestHoursWednesday"));
                latestHour = parseInt(@Model.def.getConfig("OnlineLatestHoursWednesday"));
            } else if (dayOfWeek == 4) { // Thursday
                advanceHours = parseInt(@Model.def.getConfig("OnlineAdvanceHoursThursday"));
                earliestHour = parseInt(@Model.def.getConfig("OnlineEarliestHoursThursday"));
                latestHour = parseInt(@Model.def.getConfig("OnlineLatestHoursThursday"));
            } else if (dayOfWeek == 5) { // Friday
                advanceHours = parseInt(@Model.def.getConfig("OnlineAdvanceHoursFriday"));
                earliestHour = parseInt(@Model.def.getConfig("OnlineEarliestHoursFriday"));
                latestHour = parseInt(@Model.def.getConfig("OnlineLatestHoursFriday"));
            } else if (dayOfWeek == 6) { // Saturday
                advanceHours = parseInt(@Model.def.getConfig("OnlineAdvanceHoursSaturday"));
                earliestHour = parseInt(@Model.def.getConfig("OnlineEarliestHoursSaturday"));
                latestHour = parseInt(@Model.def.getConfig("OnlineLatestHoursSaturday"));
            }

            if (isNaN(advanceHours)) { // In case valid value not set in configuration file, set default
                advanceHours = 36;
            }

            var future = Date.now() + (advanceHours * 60 * 60 * 1000);
             
            // Confirm that enough advance hours have been provided
            if(userInputInMs >= future) {
                $("#futureTime").hide();
            } else {
                $("#futureTime").text("Date must be at least " + advanceHours + " hours from now to place an order on " + weekday[dayOfWeek]).show();
            }

            // Confirm that earliest time of day is valid
            if (hourOfDay < earliestHour) {
                $("#earliestTime").text("Time must start after " + earliestHour + " O'Clock on " + weekday[dayOfWeek]).show();
            } else {
                $("#earliestTime").hide();
            }

            // Confirm that earliest time of day is valid
            if (hourOfDay > latestHour) {
                $("#latestTime").text("Time must start after " + latestHour + " O'Clock on " + weekday[dayOfWeek]).show();
            } else {
                $("#latestTime").hide();
            }
        };

        function validateZipcodes() {
            // Check that required fields are set
            if (($("#@(idPrefix)transportMethodID").val() == "") || ($("#@(idPrefix)zipcode").val() == "")) {
                console.log("empty string");
                return;
            }

            var allowableZipcode = true;
            var transportMethod = $("#@(idPrefix)transportMethodID").val();

            if (transportMethod == 29) { // Worker buses = 29
                var insideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportBusInsideZone");
                var outsideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportBusOutsideZone");

                if (insideZipCodes == "" || outsideZipCodes == "") {
                    $("#invalidZipcode").hide();
                    return;
                }

                if (insideZipCodes.indexOf(parseInt($("#@(idPrefix)zipcode"))) != -1) {
                    console.log("Within bus inside zone");
                    // Zipcode is in the allowable list of zipcodes within zone
                    allowableZipcode = true;
                } else if (outsideZipCodes.indexOf(parseInt($("#@(idPrefix)zipcode"))) != -1) {
                    console.log("Within bus outside zone");
                    // Zipcode is in the allowable list of zipcodes outside zone
                    allowableZipcode = true;
                } else {
                    console.log("NOT within bus zone");
                    allowableZipcode = false;
                }

            } else if (transportMethod == 32) { // Van service = 32
                var insideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportVanInsideZone");
                var outsideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportVanOutsideZone");

                if (insideZipCodes == "" || outsideZipCodes == "") {
                    $("#invalidZipcode").hide();
                    return;
                }

                if (insideZipCodes.indexOf(parseInt($("#@(idPrefix)zipcode"))) != -1) {
                    console.log("Within van inside zone");
                    // Zipcode is in the allowable list of zipcodes within zone
                    allowableZipcode = true;
                } else if (outsideZipCodes.indexOf(parseInt($("#@(idPrefix)zipcode"))) != -1) {
                    console.log("Within van outside zone");
                    // Zipcode is in the allowable list of zipcodes outside zone
                    allowableZipcode = true;
                } else {
                    console.log("NOT within bus outside zone");
                    allowableZipcode = false;
                }
            }

            if (!allowableZipcode) {
                $("#invalidZipcode").text("This zipcode is not a valid zipcode that we provide (van/bus) service to").show();
            } else {
                $("#invalidZipcode").hide();
            }
        }

        function setDefaultTransportationMethod() {

            var zipcode = $("#@(idPrefix)zipcode").val();

            // Check that required fields are set
            if ((transportMethod == "") || (zipcode == "")) {
                return;
            }

            var insideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportBusInsideZone");
            var outsideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportBusOutsideZone");
            if (insideZipCodes != "" && outsideZipCodes != "") {
