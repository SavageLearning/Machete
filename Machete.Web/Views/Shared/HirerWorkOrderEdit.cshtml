        function calculateTransportationFees() {
            console.log("Calculating transport fees");

            // Note: this is logic that is specific to Casa Latina only
            var workerCenter = "" + $("#workerCenter").val() + "";
            if (workerCenter != "Casa Latina") {
                return;
            }

            // Count number of workers
            var numWorkers = workAssignments.assignments.length;

            var transportMethod = $("#@(idPrefix)transportMethodID").val();
            var zipcode = $("#@(idPrefix)zipcode").val();

            // Check that required fields are set
            if ((transportMethod == "") || (zipcode == "")) {
                return;
            }

            var transportFee;
            var isTransportFeeSet = false;

            if (transportMethod == 29) { // Worker buses = 29
                var insideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportBusInsideZone");
                if (insideZipCodes != "") {
                    if (insideZipCodes.indexOf(parseInt(zipcode)) != -1) {
                        // Zipcode is in the allowable list of zipcodes within zone
                        transportFee = @Model.def.getConfig("TransportBusInsideZoneCost");
                        if (transportFee != "") {
                            isTransportFeeSet = true;
                        }
                    } 
                }
                var outsideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportBusOutsideZone");
                if (outsideZipCodes != "") {
                    console.log(outsideZipCodes);
                    if (outsideZipCodes.indexOf(parseInt(zipcode)) != -1) {
                        // Zipcode is in the allowable list of zipcodes outside zone
                        transportFee = @Model.def.getConfig("TransportBusOutsideZoneCost");
                        if (transportFee != "") {
                            isTransportFeeSet = true;
                        }
                    }
                }

            } else if (transportMethod == 32) { // Van service = 32
                var insideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportVanInsideZone");
                if (insideZipCodes != "") {
                    if (insideZipCodes.indexOf(parseInt(zipcode)) != -1) {
                        // Zipcode is in the allowable list of zipcodes within zone
                        if (numWorkers <= 1) {
                            // Note: defaulting the bus cost for just one worker before any workers are assigned & only changing value if more than one worker is assigned
                            transportFee = @Model.def.getConfig("TransportVanInsideZoneCostOneWorker");
                            if (transportFee != "") {
                                isTransportFeeSet = true;
                            }
                        } else if (numWorkers > 1) {
                            transportFee = @Model.def.getConfig("TransportVanInsideZoneCostMultipleWorkers");
                            if (transportFee != "") {
                                isTransportFeeSet = true;
                            }
                        }
                    }
                } 
                var outsideZipCodes = @Model.def.getConfig("ServiceableZipcodesTransportVanOutsideZone");
                if (outsideZipCodes != "") {
                    if (outsideZipCodes.indexOf(parseInt(zipcode)) != -1) {
                        // Zipcode is in the allowable list of zipcodes outside zone
                        transportFee = @Model.def.getConfig("TransportVanOutsideZoneCost");
                        if (transportFee != "") {
                            isTransportFeeSet = true;
                        }
                    }
                }
            }

            if (isTransportFeeSet) {
                $("#transportationFees").text("$" + transportFee.toFixed(2));
                $("#transportFee").val(transportFee.toFixed(2));
                $(".paypalWarning").show();
            } else {
                $("#transportationFees").text("n/a");
                $(".paypalWarning").hide();
                $("#transportFee").val(0);
            }

        };

